package code_section;
public class Dimond_Star_Pattern {

	public static void main(String[] args) {
		for(int i=1;i<=6;i++) {
			for(int j=1;j<=6-i;j++) {
				System.out.print(" ");
		}
			for(int k=1;k<=2*i-1;k++) {
				System.out.print(i);
			}
			System.out.println();
		}
		for(int i=5;i>=1;i--) {
			for(int j=1;j<=6-i;j++) {
				System.out.print(" ");
			}
			for(int k=1;k<=2*i-1;k++) {
				System.out.print("s");
			}
			System.out.println();
		}

	}

}
